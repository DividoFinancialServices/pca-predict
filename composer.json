{
  "name": "divido/pca-predict",
  "type": "library",
  "description": "Small library which uses Finder/Retriever PCA Predict API",
  "license": "MIT",
  "authors": [
    {
      "name": "Neil McGibbon"
    }
  ],
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "^6.5 || ^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.33",
    "phpunit/phpunit": "^9"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-4": {
      "Divido\\PCAPredict\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Divido\\Tests\\": "tests/"
    }
  }
}
